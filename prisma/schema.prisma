datasource db {
  provider = "mongodb"
  url      = env("MONGO_URI")
}

generator client {
  provider = "prisma-client-js"
}

model RssFeed {
  id        String     @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  link      String     @unique
  content   String
  image     String?
  createdAt DateTime   @default(now())
}


model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
  Hashedpassword String
}